{
  "name": "Monochrome",
  "version": "0.0.0",
  "description": "Electron based Image editing application using WebGL",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ Monochrome --platform=darwin --arch=x64 --out ~/Desktop/Monochrome --version 1.7.10 --overwrite --prune=true --icon=./assets/icons/icon.icns"
  },
  "repository": "https://github.com/Fledi1/monochrome",
  "keywords": [
    "Electron"
  ],
  "author": "CrazY",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-packager": "^10.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.3",
    "dcraw": "^1.0.3",
    "decode-tiff": "^0.2.1",
    "electron-canvas-to-buffer": "^2.0.0",
    "electron-reload": "^1.2.2",
    "jquery": "^3.2.1",
    "pngjs": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
